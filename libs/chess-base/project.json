{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/chess-base/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/chess-base",
        "tsConfig": "libs/chess-base/tsconfig.lib.json",
        "packageJson": "libs/chess-base/package.json",
        "main": "libs/chess-base/src/index.ts",
        "assets": ["libs/chess-base/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/chess-base/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/chess-base"],
      "options": {
        "jestConfig": "libs/chess-base/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
